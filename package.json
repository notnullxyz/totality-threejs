{
  "name": "totality-threejs",
  "version": "1.0.0",
  "description": "A starting workflow from superguigui's threejs starter pack",
  "main": "src/index.js",
  "repository": "",
  "scripts": {
    "start": "budo src/index.js --open --live -- -t babelify -t glslify",
    "build": "browserify src/index.js -t babelify -t glslify -p tinyify > index.js",
    "pretty": "prettier --write '{,!(node_modules)/}**/*.{js,jsx}'"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^8.3.0",
    "tinyify": "^2.3.0"
  },
  "dependencies": {
    "@superguigui/wagner": "*",
    "brindille-resize": "^0.1.3",
    "clamp": "^1.0.1",
    "glslify": "^5.0.2",
    "guigui": "^2.0.0",
    "mouse-event-offset": "^3.0.2",
    "mouse-wheel": "^1.1.0",
    "prettier": "^1.13.5",
    "raf-loop": "^1.1.3",
    "three": "*"
  },
  "author": "Guillaume Gouessan",
  "license": "BSD-2-Clause"
}
